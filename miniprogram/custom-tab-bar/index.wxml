<!--miniprogram.wxml-->
<view class="tabbar">
  <view class="tab">
    <ul class="tab-group">
      <li class="icon home-icon"></li>
      <li class="icon task-icon"></li>
    </ul>
    <ul class="tab-group">
      <li class="icon msg-icon"></li>
      <li class="icon user-icon"></li>
    </ul>
    <button class="btn" type="primary">
      <view class="add-icon"></view>
    </button>
  </view>

  <!-- 占位 -->
  <view class="danger-zone" style="height:{{tabBarBottom}}px">
    <view class="bg"></view>
  </view>
</view>
